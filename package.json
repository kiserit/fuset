{
  "name": "@kiserit/fuset",
  "version": "0.0.1",
  "description": "Express middleware to combine assets",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "private": false,
  "scripts": {
    "express": "nodemon ./test/express.js",
    "fastify": "nodemon ./test/fastify.js",
    "build": "tsup src/index.ts --format cjs,esm --dts-resolve --minify --clean --sourcemap --dts",
    "lint": "tsc",
    "dev": "vitest",
    "test": "vitest run",
    "ci": "pnpm run lint && pnpm run test && pnpm run build",
    "release": "pnpm run lint && pnpm run test && pnpm run build && changeset publish"
  },
  "keywords": [
    "assets",
    "combine",
    "combine assets",
    "combine javascript",
    "combine css",
    "express",
    "middleware"
  ],
  "author": "Kiser IT",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/mime": "^3.0.2",
    "@types/node": "18.11.9",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "mime": "^3.0.0"
  }
}
